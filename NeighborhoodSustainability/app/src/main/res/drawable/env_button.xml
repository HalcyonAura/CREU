        <shape android:shape="oval">
            <solid android:color="#0dfa09"/>
            <size android:height="200dp"/>
            <size android:width="200dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="#05c20e"/>
            <size android:height="200dp"/>
            <size android:width="200dp"/>
        </shape>
    </item>
</selector>